<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" prefHeight="500.0" prefWidth="380.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.LoginView">
   <columnConstraints>
      <ColumnConstraints maxWidth="68.0" minWidth="0.0" prefWidth="0.0" />
      <ColumnConstraints maxWidth="311.0" minWidth="243.0" prefWidth="311.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="384.0" prefWidth="346.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <Button fx:id="windowOpener" layoutX="104.0" layoutY="281.0" mnemonicParsing="false" onAction="#onClickWindowOpener" prefHeight="25.0" prefWidth="81.0" text="Войти" textAlignment="CENTER">
               <font>
                  <Font name="Microsoft JhengHei Light" size="13.0" />
               </font></Button>
            <PasswordField fx:id="passField" layoutX="61.0" layoutY="247.0" onAction="#onClickWindowOpener" promptText="Пароль">
               <font>
                  <Font name="Microsoft JhengHei Light" size="13.0" />
               </font></PasswordField>
            <TextField fx:id="usernameField" layoutX="61.0" layoutY="212.0" onAction="#onClickWindowOpener" promptText="Имя">
               <font>
                  <Font name="Microsoft JhengHei Light" size="13.0" />
               </font></TextField>
            <ImageView fitHeight="76.0" fitWidth="69.0" layoutX="114.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@logo.png" />
               </image>
            </ImageView>
            <Label layoutX="33.0" layoutY="14.0" text="BOOTS QUALITY CONTROL v.1.0">
               <font>
                  <Font name="Microsoft JhengHei Light" size="15.0" />
               </font>
            </Label>
            <Label layoutX="104.0" layoutY="181.0" text="Авторизация">
               <font>
                  <Font name="Microsoft JhengHei Light" size="13.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</GridPane>
